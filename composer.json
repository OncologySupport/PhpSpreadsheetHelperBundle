{
    "name": "oncology-support/phpspreadsheet-helper-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle ",
    "keywords": ["symfony", "phpspreadsheet", "spreadsheet", "excel", "PHPOffice"],
    "homepage": "https://github.com/ehymel/PhpSpreadsheetHelperBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernest Hymel"
        }
    ],
    "support": {
        "issues": "https://github.com/ehymel/PhpSpreadsheetHelperBundle/issues",
        "source": "https://github.com/ehymel/PhpSpreadsheetHelperBundle/releases"
    },
    "require": {
        "php": ">=8.3",
        "phpoffice/phpspreadsheet": "^4.4 || ^5.1",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/flex": "^2.8",
        "symfony/yaml": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "OncologySupport\\PhpSpreadsheetHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OncologySupport\\PhpSpreadsheetHelper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {

        }
    }
}
